{
  "name": "wcl-to-mdt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "set ANALYZE=true&&yarn build",
    "build": "next build",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn",
    "dev": "next",
    "lint:code": "eslint src --cache",
    "lint:code:fix": "eslint src --fix && prettier --write src",
    "lint:types": "tsc --noEmit",
    "start": "next start",
    "test": "exit 0",
    "postinstall": "yarn-deduplicate -l",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push --preview-feature",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:pull": "prisma db pull"
  },
  "dependencies": {
    "@prisma/client": "2.21.2",
    "next": "10.1.3",
    "next-connect": "0.10.1",
    "prisma": "2.21.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0"
  },
  "devDependencies": {
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "eslint": "7.24.0",
    "eslint-config-galex": "2.15.0",
    "ts-node": "9.1.1",
    "typescript": "4.3.0-dev.20210422",
    "yarn-deduplicate": "3.1.0"
  }
}
