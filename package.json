{
  "name": "wcl-to-mdt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "set ANALYZE=true&&yarn build",
    "build": "next build",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn",
    "dev": "next",
    "lint:code": "eslint src --cache",
    "lint:code:fix": "eslint src --fix && prettier --write src",
    "lint:types": "tsc --noEmit",
    "start": "next start",
    "test": "jest",
    "postinstall": "yarn-deduplicate -l && yarn prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push --preview-feature",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:pull": "prisma db pull"
  },
  "dependencies": {
    "@prisma/client": "2.21.2",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "next": "10.1.3",
    "next-connect": "0.10.1",
    "prisma": "2.21.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "autoprefixer": "10.2.5",
    "babel-jest": "26.6.3",
    "eslint": "7.25.0",
    "eslint-config-galex": "2.15.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.3",
    "postcss": "8.2.12",
    "prettier": "2.2.1",
    "tailwindcss": "2.1.2",
    "ts-node": "9.1.1",
    "typescript": "4.3.0-dev.20210425",
    "yarn-deduplicate": "3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/scripts/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "coverageDirectory": "coverage",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "clearMocks": true,
    "restoreMocks": true,
    "resetMocks": true
  }
}
